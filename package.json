  "main": "./dist/main.js",
  "scripts": {
    "dev:electron": "webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "webpack-dev-server --config webpack.react.config.js --mode development",
    "build:electron": "webpack --config webpack.electron.config.js --mode production",
    "build:react": "webpack --config webpack.react.config.js --mode production",
    "pack": "electron-builder --dir",
    "dist": "yarn build:react && yarn build:electron && yarn pack && electron-builder  -mw"
  },
  "dependencies": {
    "uuid": "^10.0.0"
  },
  "build": {
    "appId": "com.electron.dbanalyzer",
